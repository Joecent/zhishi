/* pages/my/cash/cash.wxss */
page{
  width: 100%;
  height: auto;
  background-color: #f9f9f9;
}
.cash{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 5%;
  font-size: 30rpx;
  background-color: #fff;
  position: relative;
  overflow: hidden;
  border-bottom: 1px solid #eeeeee;
}
.cash image{
  width:60rpx;
  height:48rpx;
  margin-top:22rpx;
  display:block;
  float:left;
  margin-right:20rpx;
}
.cash .radio-group{
  position: absolute;
  top: -4rpx;
  right: 3%;
}
radio{
  transform:scale(.8);
}
.bindweixin{
  width: 100%;
  background-color: #fff;
  padding: 20rpx 5%;
  font-size: 26rpx;
  color: #6c98dd;
}
.cashmoney{
  width: 100%;
  padding: 15px 5%;
  background-color: #fff;
  font-size: 30rpx;
  color: #555555;
  margin-top: 20rpx;
}
.cashmoney .money{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 50rpx;
  color: #ff7f50;
  overflow: hidden;
  border-bottom: 1px solid #eeeeee;
  margin-bottom: 30rpx;
  margin-top: 10rpx;
}
.cashmoney .money text{
  float: left;
}
.cashmoney .money input{
  width: 90%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #555555;
  float: left;
}
.cashmoney .keyong{
  color: #999999;
}
.prompt{
  width: 100%;
  height:100rpx;
  line-height:100rpx;
  padding: 0 5%;
  color: #cccccc;
  overflow: hidden;
  font-size: 26rpx;
}
.prompt text{
  color: #ff7f50;
  text-decoration: underline;
}
.prompt image{
  width: 28rpx;
  height: 30rpx;
  margin-top: 35rpx;
  margin-right: 20rpx;
  display: block;
  float: left;
}
.submit{
  width: 100%;
  height: auto;
}
button{
  background-color: #ff7f50;
  color: #fff;
  border: none;
}