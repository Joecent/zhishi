/* pages/my/finebalance/finebalance.wxss */
page{
  background-color: #f9f9f9;
}
.container{
  width: 100%;
  padding: 0 5%;
  background-color: #fff;
  margin-top: 20rpx;
}
.fine{
  width: 100%;
  height: auto;
  padding: 10rpx 0;
  border-bottom: 1px solid #eeeeee;
}
.fine .balance{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
}
.fine .balance .tixian{
  width: 60%;
  float: left;
  font-size: 30rpx;
  color: #444444;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.fine .balance .tixian_money{
  float: right;
  font-size: 30rpx;
  color: #444444;
}
.fine .balance .time{
  float: left;
  font-size: 26rpx;
  color: #666666;
}
.fine .balance .time_money{
  float: right;
  font-size: 26rpx;
  color: #666666;
}
.none{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  text-align: center;
}