/* pages/my/published/published.wxss */
.classification{
  width: 100%;
  height: auto;
  margin-top: 20rpx;
}
.classification .fixed{
  width: 90%;
  position: fixed;
  top: 0;
  left: 5%;
  z-index: 9999;
  background-color: #fff;
}
.classification .fixed .search{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  border: 1px solid #b3b3b3;
  border-radius: 5px;
  position: relative;
}
.classification .fixed .search image{
  width: 40rpx;
  height: 40rpx;
  display: block;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}
.classification .fixed .search input{
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 10%;
  font-size: 30rpx;
}
.classification .title{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
}
.classification .title .title_name{
  font-size: 30rpx;
  color: #484848;
  float: left;
}
.classification .search{
 width: 100%;
 height: auto;
 border-bottom: 1px solid #eeeeee;
}
.classification .search input{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}
.classification .productalone{
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 62rpx;
}
.classification .outside{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.classification .none{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  text-align: center;
}
.outside .product{
  width: 100%;
  padding: 50rpx 0;
  border-bottom: 1px solid #eeeeee;
  overflow: hidden;
}
.outside .product image{
  width: 138rpx;
  height: 184rpx;
  display: block;
  float: left;
}
.outside .book_det{
  float: left;
  width: 72%;
  margin-left: 20rpx;
}
.outside .book_det .book_name{
  font-size: 32rpx;
  color: #484848;
  margin-bottom: 15rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.outside .book_det .book_introduce{
  font-size: 26rpx;
  color: #999999;
  margin-bottom: 10rpx;
  overflow : hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.outside .book_det .book_press{
  width: 100%;
  height: 65rpx;
  overflow: hidden;
  line-height: 65rpx;
}
.book_press text{
  font-size: 26rpx;
  margin-right: 5rpx;
  float: left;
  margin: 0 0;
}
.book_press .press_name{
  color: #a2b3ce;
}
.book_press .press_address{
  color: #a2b3ce;
}
.book_press .press_price{
  color: #ed7a50;
  margin-right: 10rpx;
  font-size: 34rpx;
}
.book_press .press_price_y{
  color: #c4c4c4;
  text-decoration:line-through; 
}
.book_press .stock{
  float: right;
}
.book_press .cancel{
  width: auto;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 14rpx;
  margin-top: 12.5rpx;
  border: 1px solid #c4c4c4;
  border-radius: 10rpx;
  color: #c4c4c4;
  z-index: 9999;
}