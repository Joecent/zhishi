/* pages/mine/orderdetails/orderdetails.wxss */
.title_name{
  width: 100%;
  height: auto;
  font-size: 36rpx;
  color: #484848;
  margin-bottom: 20rpx;
}
.name_phone{
  width: 100%;
  height: auto;
  font-size: 30rpx;
  color: #484848;
  margin-bottom: 20rpx;
}
.name_phone text{
  margin-right: 20rpx;
}
.name_phone .phone{
  color: #6c98dd;
}
.location{
  width: 100%;
  height: auto;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #eeeeee;
  overflow: hidden;
}
.location image{
  width: 26rpx;
  height: 32rpx;
  display: block;
  float: left;
  margin-right: 20rpx;
}
.location text{
  width: 40%;
  float: left;
  font-size: 28rpx;
  color: #787878;
}
.details{
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 30rpx 0;
  border-bottom: 1px solid #eeeeee;
  position: relative;
}
.details image{
  width: 138rpx;
  height: 184rpx;
  display: block;
  float: left;
  margin-right: 20rpx;
}
.details .name_author{
  width: 60%;
  height: auto;
  float: left;
  font-size: 30rpx;
}
.details .name_author text{
  width: 100%;
  height: auto;
  float: left;
  font-size: 30rpx;
   overflow : hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.details .number{
  position: absolute;
  bottom: 35rpx;
  left: 158rpx;
  width: 78%;
  height: auto;
  float: left;
  font-size: 26rpx;
}
.details .number text{
  margin-right: 40rpx;
}
.details .price{
  position: absolute;
  top: 30rpx;
  right: 0;
  font-size: 28rpx;
  color: #ed7a50;
}
.details .num{
  position: absolute;
  top: 80rpx;
  right: 0;
  font-size: 28rpx;
}
.time_logistics{
  width: 100%;
  height: auto;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #eeeeee;
  position: relative;
}
.time_logistics .price{
  color: #ed7a50;
}
.time_logistics .free{
  margin-right: 140rpx;
  color: #787878;
}
.time_logistics text{
  width: 100%;
  height: auto;
  display: block;
  font-size: 28rpx;
  margin-top: 30rpx;
}
.time_logistics view{
  width: 100%;
  height: auto;
  display: block;
  font-size: 28rpx;
  margin-top: 30rpx;
  position: relative;
}
.time_logistics view text{
  width: auto;
  height: auto;
  position: absolute;
  right: 0;
  top: 0;
  margin-top: 0;
}
.time_logistics .totel{
  text-align: right;
  font-size: 32rpx;
}
.time_logistics .totel text{
  width: auto;
  height: auto;
  margin-top: 0;
  color: #ed7a50;
  display: inherit;
  font-size: 32rpx;
}
.time_logistics .looklogistics{
  background-color:#6c98dd;
  color:#fff;
  padding:5rpx 10rpx;
  border-radius:10rpx;
  width:120rpx;
  text-align:center;
  height:40rpx;
  line-height:30rpx;
  font-size:25rpx;
  position:absolute;
  top:30rpx;
  right:0;
}
.m_top{
  margin-top: 20rpx;
}