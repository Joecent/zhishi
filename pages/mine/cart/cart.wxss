/* pages/mine/cart/cart.wxss */
page{
  min-height: 100%;
  background-color: #fff;
}
.container {
  background-color: #fff;
  min-height: 100%;
  padding: 0;
}
.pos-fiexd{
  position: fixed;
  bottom: 0;
  left: 0;
}
.container{
    justify-content:initial;
}
.to-index-btn{
    color:#fff;
    background:#e64340;
    border-radius:6px;
    width:300rpx;
    height:70rpx;
    line-height: 70rpx;
    text-align: center;
    font-size:28rpx;
}
.list-top{
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    box-sizing: border-box;
    display: flex;
    justify-content:space-between;
    padding: 0 30rpx;
    font-size: 28rpx;
    align-items: center;
    background-color: #fff;
    border-top: 1px solid #dee4ec;
    border-bottom: 1px solid #dee4ec;
}
.list-top .label{
    color: #000;
}
.list-top .edit-btn{
    color: #0084fb;
    height: 100%;
}
.goodsList{
    width: 100%;
    background-color: #fff;
    margin-bottom: 120rpx;
}
.a-gooods{
    width: 100%;
    overflow: hidden;
}
.a-goods-conts{
    display: flex;
    justify-content:space-between;
    padding-left: 60rpx;
    width: 870rpx;
    box-sizing: border-box;
    transition: margin-left 0.2s ease-in-out;
    background: url(data:image/png;base64,) no-repeat 0rpx center;
    background-size: 40rpx auto; 
    position: relative;
}
.a-goods-conts.active{
    background: url(data:image/png;base64,) no-repeat 0rpx center;
    background-size: 40rpx auto; 
}
.goods-info{
    border-bottom: 1px solid #dee4ec;
    display: flex;
    justify-content:space-between;
    padding: 30rpx 0 30rpx 30rpx; 
    width: 720rpx;
    box-sizing: border-box;
}
.goods-info .img-box{
    width: 138rpx;
    height: 184rpx;
    overflow: hidden;
    margin-right: 20rpx;
    background-color: #d8d8d8;
    position: relative;
}
.goods-info .text-box{
    width: 500rpx;
    height: 140rpx;
    position: relative;
}
.goods-info .text-box .goods-title{
    font-size: 28rpx;
    color:#000;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    padding: 10rpx 20rpx 5rpx 0;
}
.goods-info .text-box .goods-label{
    font-size: 26rpx;
    color:#999;
    height: 80rpx;
    line-height: 38rpx;
    margin: 8rpx 0 0 0;
    overflow: hidden;
}
.goods-info .text-box .goods-price{
    font-size: 26rpx;
    color:#e64340;
}
.goods-info .text-box .buy-num{
    width: 164rpx;
    height: 48rpx;
    line-height: 48rpx;
    position: absolute;
    right: 30rpx;
    bottom: 0;
    display: flex;
    /*justify-content: space-between;*/
    font-size: 24rpx;
    text-align: center
}
.goods-info .text-box .buy-num .jian-btn{
    width: 48rpx;
    height: 100%;
    border-left: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    border-top: 1rpx solid #ccc;
    border-bottom-left-radius: 6rpx;
    border-top-left-radius: 6rpx;
}
.goods-info .text-box .buy-num .jian-btn.disabled{
    background-color: #f5f5f9;
    border-left: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    color: #ccc;
}
.goods-info .text-box .buy-num .jia-btn{
    width: 48rpx;
    height: 100%;
    border-right: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    border-top: 1rpx solid #ccc;
    border-bottom-right-radius: 6rpx;
    border-top-right-radius: 6rpx;
}
.goods-info .text-box .buy-num .jia-btn.disabled{
    background-color: #f5f5f9;
    border-right: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    color: #ccc;
}
.goods-info .text-box .buy-num input{
    width: 68rpx;
    height: 48rpx;
    min-height: 48rpx;
    text-align: center;
    font-size: 24rpx;
    border: 1rpx solid #ccc;
}

.goods-info .img-box .img{
    width: 138rpx;
    height: 184rpx;
}

.a-goods-conts .delete-btn{
    width: 120rpx;
    line-height: 200rpx;
    text-align: center;
    background:#e64340;
    font-size: 24rpx;
    color: #fff;
}
.jiesuan-box{
    display: flex;
    justify-content: space-between;
    width: 95%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 5%;
    border-bottom:1px solid #eee; 
    border-top:1px solid #eee; 
    background-color: #fff;
    z-index: 9999;
}
.jiesuan-box .to-pay-btn{
    width:240rpx;
    text-align: center;
    line-height: 100rpx;
    background-color: #ff7f50;
    font-size:32rpx;
    color:#ffffff;
}
.jiesuan-box .to-pay-btn.no-select{
    background-color: #ccc;
}
.jiesuan-box .to-del-btn{
    width:240rpx;
    text-align: center;
    line-height: 100rpx;
    background-color: #EE0D42;
    font-size:32rpx;
    color:#ffffff;
}
.jiesuan-box .to-del-btn.no-select{
    background-color: #ccc;
}

.jiesuan-box  .left-price{
    display: flex;
    width: 150rpx;
    justify-content: space-between;
    line-height: 100rpx;
    padding: 0 30rpx 0 0;
    font-size:28rpx;
    box-sizing: border-box;
}
.jiesuan-box .all-selected{
    padding-left: 60rpx;
    color:#000000;
    background: url(data:image/png;base64,) no-repeat 0rpx center;
    background-size: 40rpx auto; 
}
.jiesuan-box .all-selected.active{
    background: url(data:image/png;base64,) no-repeat 0rpx center;
    background-size: 40rpx auto; 
}
.jiesuan-box .total{
    color: #ed7a50;
}
.jiesuan-box .total_price{
  font-size: 30rpx;
  line-height: 100rpx;
  color: #484848;
  float: right;
}

.base{
  width: 100%;
  height: 100rpx;
  background-color: #f8f8f8;
}

.inquiry{
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
}
.inquiry .nav_icon{
    width: 49.8%;
    height: 60rpx;
    line-height: 60rpx;
    margin-top: 20rpx;
    float: left;
    font-size: 28rpx;
    text-align: center;
    color: #0084fb;
    
}
.inquiry .nav_icon image{
    width: 50rpx;
    height: 50rpx;
    float: left;
    margin-top: 5rpx;
    margin-left: 90rpx;
    margin-right: 20rpx;
}
.inquiry .nav_icon text{
    float: left;
}
.goodsList .art{
    width: 100%;
    height: auto;
    background-color: #000;
    opacity: 0.8;
    text-align: center;
    color: #fff;
    position: absolute;
    top: 0;
    left: 0;
    font-size: 20rpx;
    padding: 0;
    display: none;
}
.goodsList .block{
    display: block;
}
.title_name{
  width: 100%;
  height: auto;
  font-size: 40rpx;
  color: #484848;
  margin-bottom: 20rpx;
}
.select{
  width: 100%;
  height: auto;
  position: inherit;
  border-top: none;
}
.select .length{
  color: #787878;
  font-size: 30rpx;
  line-height: 100rpx;
}
.details{
  width: 75%;
  height: auto;
  overflow: hidden;
  padding: 20rpx 0;
  border-bottom: 1px solid #eeeeee;
  position: relative;
}
.details image{
  width: 138rpx;
  height: 184rpx;
  display: block;
  float: left;
  margin-right: 20rpx;
}
.details .name_author{
  width: 60%;
  height: auto;
  float: left;
  font-size: 30rpx;
}
.details .name_author text{
  width: 100%;
  height: auto;
  float: left;
  font-size: 24rpx;
   overflow : hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #999999;
  margin-bottom: 10rpx;
}
.details .name_author .title_details{
  width: 100%;
  height: auto;
  float: left;
  font-size: 30rpx;
   overflow : hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  color: #484848;
}
.details .number{
  position: absolute;
  bottom: 20rpx;
  left: 158rpx;
  width: 78%;
  height: auto;
  float: left;
  font-size: 24rpx;
}
.details .number text{
  margin-right: 40rpx;
}
.details .price{
  position: absolute;
  top: 20rpx;
  right: 0;
  font-size: 28rpx;
  color: #ed7a50;
}
.details .num{
  position: absolute;
  top: 140rpx;
  right: 0;
  line-height: 60rpx;
  font-size: 30rpx;
}
.details .num .noclick{
  color:#a3a2a2;
}
.details .num text{
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  font-size: 50rpx;
  float: left;
}
.details .num input{
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #ed7a50;
  display: block;
  float: left;
}
.select_button{
  width: 40rpx;
  height: 184rpx;
  background-color: none;
  position: absolute;
  top: 20rpx;
  left: 0;
}